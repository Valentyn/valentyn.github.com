<div id="client_side_database_size">
	<div id="header">
		<div class="topic"><%= title %></div>
	</div>
	<div class="content">
		<table>
			<% _.each(content, function (index) { %>
				<tr class="description">
					<td><%= _.keys(index) %></td> 
					<td><%= _.values(index) %></td>
				</tr>
			<% }); %>			
		</table>
	</div>	
	<div id="orange_line"></div>
	<div id="blue_line_bottom"></div>	
	<div id="footer"></div>		
</div>