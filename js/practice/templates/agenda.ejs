<div id="offline_web_app_agenda">
	<div id="header">
		<div class="topic"><%= title %></div>
	</div>
	<div class="content">
		<ol type="I">
			<% _.each(content, function (item) { %>
				<li><%= item %></li>
			<% }); %>
		</ol>
		<div class="notebook"></div>
	</div>
	<div id="orange_line"></div>
	<div id="blue_line_bottom"></div>	
	<div id="footer"></div>
</div>